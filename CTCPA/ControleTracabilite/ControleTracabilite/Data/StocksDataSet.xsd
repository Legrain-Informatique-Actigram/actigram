                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="False">
                    <CommandText>Select distinct codeproduit,nlot ,m.DateMouvement as Dt,m.TypeMouvement as tp,m.Description
from mouvement_detail md
inner join mouvement m on m.nmouvement=md.nmouvement
where nlot is not null and codeproduit is not null 
and (m.TypeMouvement ='Entrées' 
	OR m.TypeMouvement ='Fabrication' 
	OR (m.TypeMouvement='Conditionnement' AND unite1&gt;0))
UNION
Select distinct codeproduit,nlot ,br.datefacture as Dt,'Réception' as tp,e.Nom
from abonreception_detail brd
inner join abonreception br on brd.ndevis=br.ndevis
inner join entreprise e on br.nclient=e.nentreprise
where nlot is not null and codeproduit is not null</CommandText>
