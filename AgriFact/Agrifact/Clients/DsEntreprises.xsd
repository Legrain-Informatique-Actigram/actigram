                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="False">
                    <CommandText>SELECT        'F' AS Type, nDevis AS Id, DateFacture AS Date, (CASE WHEN montantTTC &lt; 0 THEN 'Avoir' ELSE 'Facture' END) + ' n°' + CONVERT(varchar(10), nFacture) 
                         AS libelle, MontantTTC AS montant, Paye AS traite
FROM            VFacture
WHERE        (nClient = @nEntreprise) AND DateFacture &gt;=@DtDeb AND DateFacture &lt;@DtFin
UNION
SELECT        'R' AS Type, nReglement AS Id, DateReglement AS Date, 'Régl ' + nMode + ISNULL(nCheque, '') AS libelle, - Montant AS montant, Depose AS traite
FROM            Reglement
WHERE        (nEntreprise = @nEntreprise) AND DateReglement &gt;=@DtDeb AND DateReglement &lt;@DtFin ORDER BY Date</CommandText>
