                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="False">
                    <CommandText>SELECT        'R' AS Type, r.nReglement AS id, r.nMode + ISNULL(' n°' + r.Observation, '') AS Libelle, r.DateReglement AS date, r.Montant, r.Montant - SUM(ISNULL(rd.Montant, 0) 
