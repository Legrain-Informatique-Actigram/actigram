FROM            VFacture AS f LEFT OUTER JOIN
                         Reglement_Detail AS rd ON f.nDevis = rd.nFacture
WHERE        (f.MontantTTC &lt; 0) AND (f.nClient = @nClient)
GROUP BY f.nDevis, f.nFacture, f.DateFacture, f.MontantTTC
HAVING        (f.MontantTTC - SUM(ISNULL(rd.Montant, 0)) &lt;&gt; 0)</CommandText>
