                         Reglement_Detail AS rd ON r.nReglement = rd.nReglement
WHERE        (r.nEntreprise = @nClient)
GROUP BY r.nReglement, r.nMode, r.Observation, r.DateReglement, r.Montant
HAVING        (r.Montant - SUM(ISNULL(rd.Montant, 0) - ISNULL(rd.Perte, 0) + ISNULL(rd.Profit, 0) - ISNULL(r.MontantEscompte, 0)) &lt;&gt; 0)
UNION
